      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.1",

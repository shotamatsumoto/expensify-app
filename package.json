    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.4.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "identity-obj-proxy": "^3.0.0",
    "moment": "^2.22.1",
    "node-sass": "^4.8.3",
    "normalize.css": "^8.0.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",

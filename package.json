    "react-with-styles": "^3.2.0",
    "react-with-styles-interface-aphrodite": "^5.0.0",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uuid": "^3.2.1",
    "validator": "^8.0.0",
    "webpack": "^3.1.0"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.0.1",
    "live-server": "^1.2.0",

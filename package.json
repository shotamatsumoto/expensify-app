  "license": "MIT",
  "scripts": {
    "build:dev": "webpack --watch",
    "build:prod": "webpack -p --env production",
    "dev-server": "webpack-dev-server",
    "test": "jest --config=jest.config.json --watchAll",
    "start": "node server/server.js",
    "heroku-postbuild": "npm run build:prod"
